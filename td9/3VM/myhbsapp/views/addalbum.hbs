<h3>Add Album</h3>
{{#if error}} <span>{{error}}</span>{{/if}}
<form method="POST" action="/album/add">
  <div class="form-group">
    <label>ID</label>
    <input type="text" id="id" name="id" class="form-control" placeholder="Add ID">
  </div>
  <div class="form-group">
    <label>Titre</label>
    <input type="text" id="titre" name="Titre" class="form-control" placeholder="Titre">
  </div>
  <div class="form-group">
    <label>Description</label>
    <input type="text" id="description" name="Description" class="form-control" placeholder="Description">
  </div>
  <div class="form-group">
    <label>Photo1</label>
    <input type="text" id="photo1" name="photo1" class="form-control" placeholder="Photo1 Name">
  </div>
  <div class="form-group">
    <label>Photo2</label>
    <input type="text" id="photo2" name="photo2" class="form-control" placeholder="Photo2 name">
  </div>
  <div class="form-group">
    <label>Photo3</label>
    <input type="text" id="photo3" name="photo3" class="form-control" placeholder="Photo3 name">
  </div>
  <div class="form-group">
    <label>Photo4</label>
    <input type="text" id="photo4" name="photo4" class="form-control" placeholder="Photo4 name">
  </div>
  <input type="submit" id="submit" class="btn btn-primary" value="Save">
</form>
